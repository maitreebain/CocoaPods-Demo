# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'CocoaPods-Demo' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for CocoaPods-Demo
  
  #Alamofire networking library
  pod 'Alamofire' #not specifiying the version number will install the most recent version of the library
  
  #SnapKit is an AutoLayout third party library
  pod 'SnapKit'
  
  # Kingfisher is a photo processing library
  pod 'Kingfisher'
  
end
